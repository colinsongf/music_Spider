{
	"damaiwang":[{
			"Index_Url":"http://www.damai.cn/ajaxArtist.aspx?type=5&pageIndex=1",
			"Max_Page":{
				"soup":"",
				"re":"^(\\d+)?"
			}
		},
		{
			"All_Detail_Page":{
				"xpath":"//a[@class=\"f14\"]/@href"
			}
		},
		{
			"Signal_Detail_Page":{
				"xpath":"//ul[@class=\"art_menu\"]/li[3]/a/@href",
				"Some_Info":{
					"performer":"//strong[@class=\"f22\"]/text()"
				}
			}
		},
		{
			"Target_Detail_Page":{
				"xpath":"//div[@class=\"art_ticket\"]/ul/li/h3/a/@href"
			}
		},
		{
			"Final_Xpath":{
				"concert_title":"//h2[@class=\"tt\"]/span/text()",
				"concert_place":"//div[@class=\"ct\"]/p[@class=\"txt\"]/a/text()",
				"concert_time":"//div[@class=\"ct\"]/ul[@class=\"lst \"]/li[@class=\"itm itm-sel\"]/a/text()",
				"concert_price":"//div[@class=\"ct\"]/ul[@class=\"lst \"]//li/@data-pricename"
			}
		}],
	"qq_music":[{
			"Index_Url":"https://y.qq.com/portal/singerlist.html#t4=1",
			"Max_Page":{
				"soup":"",
				"re":"(?<=total_page : \\')\\d+"
			}
		},
		{
			"All_Detail_Page":{
				"xpath":["//ul[@class=\"singer_list__list\"]/li/div/a/@href","//ul[@class=\"singer_list_txt\"]/li/a/@href"]
			}
		},
		{
			"Final_Xpath":{
					"music_counts":"//ul[@class=\"mod_data_statistic\"]/li[@class=\"data_statistic__item\"][1]/a/strong/text()",
					"album_counts":"//ul[@class=\"mod_data_statistic\"]/li[@class=\"data_statistic__item\"][2]/a/strong/text()",
					"mv_counts":"//ul[@class=\"mod_data_statistic\"]/li[3]/a/strong/text()",
					"concern_counts":"//a[@class=\"mod_btn js_follow\"]/text()"
			}		
		}],
	"qq_music_mv":[{
			"Index_Url":"https://y.qq.com/portal/mv_lib.html?t5=1",
			"Max_Page":{
					"xpath":"//div[@class=\"mod_page_nav js_pager\"]/a[4]/text()"
			}
		},
		{
			"Final_Xpath":{
					"All_Xpath":"//div[@class=\"mv_list__item_box\"]",
					"mv_title":"h3/a/text()",
					"artist_name":"div[@class=\"mv_list__singer\"]/a/text()",
					"play_counts":"div[@class=\"mv_list__info\"]/span/text()",
					"publish_time":"div[@class=\"mv_list__info\"]/text()",
					"mv_mid":"a/@data-vid"
			}
		}],
	"xiami_music":[{
			"Index_Url":"http://www.xiami.com/artist/index/c/1/type/0/class/0/page/1",
			"Max_Page":{
					"xpath":"//div[@class=\"all_page\"]/span/text()",
					"re":"\\d\\d\\d"
				}
			},
		{
			"All_Detail_Page":{
					"xpath":["//div[@class=\"artist\"]/div/a/@href"]
			}
		},
		{
			"Final_Xpath":{
					"artist_name":["//div[@id=\"title\"]/h1/text()","//div[@class=\"clearfix\"]/h1/text()"],
					"play_counts":["//em[@id=\"play_count_num\"]/text()"],
					"fans_counts":["//div[@class=\"music_counts\"]/ul[@class=\"clearfix\"]/li[2]/a/text()"],
					"comment_counts":["//div[@class=\"music_counts\"]/ul[@class=\"clearfix\"]/li[3]/a/text()"]
			}
		}]

}
